class Solution {
    public int reverse(int x) {
        int temp;
        temp=x;
        int rev=0;
        
        while(x!=0){
           int rem= x%10;
            rev=rev*10+rem;
            x=x/10;
             if(x!=0 && (rev>214748364 || rev<-214748364) )
            {
                return 0;
            }
        }  return rev;
        } 
    }
